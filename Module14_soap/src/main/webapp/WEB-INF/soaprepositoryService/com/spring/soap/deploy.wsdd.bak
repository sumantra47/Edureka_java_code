<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from soaprepositoryService WSDL service -->

  <service name="soaprepository" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://soap.spring.com"/>
      <parameter name="wsdlServiceElement" value="soaprepositoryService"/>
      <parameter name="schemaQualified" value="http://soap.spring.com"/>
      <parameter name="wsdlServicePort" value="soaprepository"/>
      <parameter name="className" value="com.spring.soap.SoaprepositorySoapBindingImpl"/>
      <parameter name="wsdlPortType" value="soaprepository"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="addnum" qname="operNS:addnum" xmlns:operNS="http://soap.spring.com" returnQName="retNS:addnumReturn" xmlns:retNS="http://soap.spring.com" returnType="rtns:int" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:a" xmlns:pns="http://soap.spring.com" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:b" xmlns:pns="http://soap.spring.com" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="subnum" qname="operNS:subnum" xmlns:operNS="http://soap.spring.com" returnQName="retNS:subnumReturn" xmlns:retNS="http://soap.spring.com" returnType="rtns:int" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:a" xmlns:pns="http://soap.spring.com" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:b" xmlns:pns="http://soap.spring.com" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="addnum subnum"/>

  </service>
</deployment>
